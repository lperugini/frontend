<% orders.forEach(function(order) { %>
<div class="card" style="width: 18rem">
  <div class="card-body">
    <a href="orders/<%= order.id %>"
      ><h5 class="card-title"><%= order.code %></h5>
    </a>
    <p class="card-text"><%= order.userInfo.firstName + ' ' + order.userInfo.lastName %></p>
    <p class="card-text"><%= order.itemInfo.name %></p>
    <p class="card-text"><%= order.price %>â‚¬</p>
  </div>
</div>
<% }); %>
